<%- include("partials/header.ejs") %>
<div class="blogs">
    <% if(locals.blogs){ %>
        <div>
            <a href="">
                <% blogs.forEach((blog) =>{ %>
                    <div class="individual-blog">
                        <h1><%= blog.title  %></h1>
                        <p> <%= blog.description %> </p>
                    </div>
                <% }) %> 
            </a>
        </div>
    <% }  %>
    <a href="/add"><input type="button" value="+"></a>
</div>




