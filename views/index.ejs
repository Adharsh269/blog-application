<%- include("partials/header.ejs") %>
        <div class="blogs">
            <% if(locals.blogs){ %>
                <div class="inner">
                    <% blogs.forEach((blog)=>{ %>
                            <div class="individual-blog">
                                <div class="heading">
                                    <h1><%= blog.title %></h1>
                                </div>
                                <div class="explain">
                                    <p><%= blog.description %></p>
                                </div>
                            </div>
                    <% }); %>
                    <a href="/add" id="add"><img src="/images/plus.png" alt="add" id="img"></a>
                </div>
                <% } %>
                
        </div>
    <%- include("partials/footer.ejs") %>